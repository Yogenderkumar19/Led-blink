// Gas sensor without buzzer 
void setup() {
 Serial.begin(9600);
}

void loop() {
int sensorValue = analogRead(A0);
Serial.println(sensorValue);
delay(500);
}

// Gas sensor with buzzer

#define BUZZER 3

void setup() {
pinMode(BUZZER, OUTPUT);
 Serial.begin(9600);
}

void loop() {
int sensorValue = analogRead(A0);
Serial.println(sensorValue);
if (sensorValue>200)
{analogWrite(BUZZER, 50);
}else
{analogWrite(BUZZER,0);
}
delay(500);
}
// BUZZER Pin GND, Pin = 3
